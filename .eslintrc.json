{
    "env": {
        "browser": true,
        "jquery": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "sourceType": "module"
    },
    "globals": {
        "$p": true,
        "projekktorMessages": true
    },
    "rules": {
        "no-const-assign": "warn",
        "no-this-before-super": "warn",
        "no-undef": "warn",
        "no-unreachable": "warn",
        "no-unused-vars": [
            "warn",
            {
                "args": "none"
            }
        ],
        "constructor-super": "warn",
        "valid-typeof": "warn"
    }
}